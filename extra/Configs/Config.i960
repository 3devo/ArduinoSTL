config TARGET_ARCH
	default "i960"

config HAVE_ELF
	bool
	select HAS_NO_THREADS
	default n

config ARCH_CFLAGS
	string
	default "-mh -mint32 -fsigned-char"

config ARCH_LDFLAGS
	string

config LIBGCC_CFLAGS
	string
	
config ARCH_HAS_C_SYMBOL_PREFIX
	bool
	default y

